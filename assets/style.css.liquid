*,
*:before,
*:after {
  margin: 0;
  padding: 0;
  box-sizing: border-box;

}

.upper-header_text {
  font-size: 14px;
  color: white;
}

.upper-header {
  background-color: #11346F;
}

.lower_header {
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.sticky {
  position: fixed;
  top: 0;
  width: 100%;
}